{
  "name": "@avernikoz/memechan-sol-sdk",
  "description": "Typescript SDK for memechan.gg",
  "version": "1.0.44",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "node": "v20",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "test": "jest",
    "audit": "yarn audit --groups dependencies",
    "format": "prettier --write src/**/*.ts",
    "lint": "tsc --noEmit && eslint src/**/*.ts",
    "build": "yarn rimraf dist && yarn lint && tsup",
    "build:watch": "tsc --watch",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "codegen": "yarn run rimraf src/schema/codegen/ && yarn run anchor-client-gen src/schema/idl/memechan_sol.json src/schema/codegen/",
    "deploy": "yarn lint && yarn build && yarn publish --access restricted"
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "^5.2.0",
    "@aws-sdk/types": "^3.577.0",
    "@coral-xyz/anchor": "0.29.0",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@metaplex-foundation/umi": "^0.9.1",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.52",
    "@smithy/protocol-http": "^4.0.0",
    "@smithy/signature-v4": "^3.0.0",
    "@solana/spl-token": "^0.4.6",
    "@solana/web3.js": "^1.91.8",
    "bignumber.js": "^9.1.2",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "dotenv": "^16.4.5",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.5",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "anchor-client-gen": "0.24.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5"
  },
  "keywords": [],
  "author": "0xunreal",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/memechan-gg/memechan-sol.git"
  },
  "bugs": {
    "url": "https://github.com/memechan-gg/memechan-sol/issues"
  },
  "homepage": "https://github.com/memechan-gg/memechan-sol#readme"
}
